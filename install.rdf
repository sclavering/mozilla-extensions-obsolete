<?xml version="1.0"?>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns="http://www.mozilla.org/2004/em-rdf#">

  <rdf:Description about="urn:mozilla:install-manifest">
    <!-- GUIDs are ugly, and don't actually solve the problem they're supposed to. -->
    <id>org.mozdev.clav.tabx</id>

    <name>Tab X</name>
    <version>0.2</version>
    <description>Adds a close button to each of the browser tabs</description>

    <creator>Stephen Clavering</creator>
    <homepageURL>http://clav.mozdev.org/</homepageURL>

    <targetApplication>
      <rdf:Description>
        <id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</id>
        <minVersion>0.8</minVersion>
        <maxVersion>0.9</maxVersion>
      </rdf:Description>
    </targetApplication>

    <file>
      <rdf:Description about="urn:mozilla:extension:file:tabx.jar">
        <package>content/</package>
        <skin>skin/</skin>
      </rdf:Description>
    </file>
  </rdf:Description>
</rdf:RDF>
