<?xml-stylesheet href="chrome://linktoolbar/skin/linkToolbar.css" type="text/css"?>

<!DOCTYPE overlay SYSTEM "chrome://linktoolbar/locale/linkToolbar.dtd">

<overlay id="linkToolbarOverlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script type="application/x-javascript" src="linkFinder.js"/>
  <script type="application/x-javascript" src="linkToolbarHandler.js"/>
  <script type="application/x-javascript" src="linkToolbarItem.js"/>
  <script type="application/x-javascript" src="linkToolbarUI.js"/>

  <!-- our two line tooltip -->
  <popupset id="mainPopupSet">
    <tooltip id="linktoolbar-tooltip" orient="vertical"
        onpopupshowing="return linkToolbarFillTooltip(this, event);">
      <label id="linktoolbar-tooltip-1"/>
      <label id="linktoolbar-tooltip-2" crop="center"/>
    </tooltip>
  </popupset>

<keyset id="mainKeyset">
  <key id="linktoolbar-key-first" key="&key.first;" modifiers="&modifiers.first;"
      oncommand="linkToolbarGo('first');"/>
  <key id="linktoolbar-key-prev" key="&key.prev;" modifiers="&modifiers.prev;"
      oncommand="linkToolbarGo('prev');"/>
  <key id="linktoolbar-key-next" key="&key.next;" modifiers="&modifiers.next;"
      oncommand="linkToolbarGo('next');"/>
  <key id="linktoolbar-key-last" key="&key.last;" modifiers="&modifiers.last;"
      oncommand="linkToolbarGo('last');"/>
</keyset>

  <statusbar id="status-bar">
    <statusbarpanel id="linktoolbar" hasitems="false"
        oncommand="return linkToolbarLoadPage(event, false);"
        onclick="return event.button==1 &amp;&amp; linkToolbarLoadPage(event, true);"
        insertafter="livemark-button" tooltip="linktoolbar-tooltip"
        >

      <toolbarbutton disabled="true" type="menu-button" class="linktoolbar-button"
          id="linktoolbar-top" label="&top.label;" tooltiptext0="&top.tooltip;"/>
      <toolbarbutton disabled="true" type="menu-button" class="linktoolbar-button"
          id="linktoolbar-up" label="&up.label;" tooltiptext0="&up.tooltip;"/>

      <toolbarbutton disabled="true" type="menu-button" class="linktoolbar-button"
          id="linktoolbar-first" label="&first.label;" tooltiptext0="&first.tooltip;"/>
      <toolbarbutton disabled="true" type="menu-button" class="linktoolbar-button"
          id="linktoolbar-prev" label="&prev.label;" tooltiptext0="&prev.tooltip;"/>
      <toolbarbutton disabled="true" type="menu-button" class="linktoolbar-button"
          id="linktoolbar-next" label="&next.label;" tooltiptext0="&next.tooltip;"/>
      <toolbarbutton disabled="true" type="menu-button" class="linktoolbar-button"
          id="linktoolbar-last" label="&last.label;" tooltiptext0="&last.tooltip;"/>

      <toolbarbutton disabled="true" type="menu"
          id="linktoolbar-more-menu" label="&more.label;" tooltiptext0="&more.tooltip;">
        <menupopup id="linktoolbar-more-popup">
          <menuitem id="linktoolbar-toc" label="&toc;" hidden="true" class="menuitem-iconic"/>
          <menu id="linktoolbar-chapter" label="&chapters;" hidden="true" class="menu-iconic"/>
          <menu id="linktoolbar-section" label="&sections;" hidden="true" class="menu-iconic"/>
          <menu id="linktoolbar-appendix" label="&appendices;" hidden="true" class="menu-iconic"/>
          <menuitem id="linktoolbar-glossary" label="&glossary;" hidden="true" class="menuitem-iconic"/>
          <menuitem id="linktoolbar-index" label="&index;" hidden="true" class="menuitem-iconic"/>

          <menuitem id="linktoolbar-help" label="&help;" hidden="true" class="menuitem-iconic"/>
          <menuitem id="linktoolbar-search" label="&search;" hidden="true" class="menuitem-iconic"/>
          <menuitem id="linktoolbar-author" label="&author;" hidden="true" class="menuitem-iconic"/>
          <menuitem id="linktoolbar-copyright" label="&copyright;" hidden="true" class="menuitem-iconic"/>
          <menu id="linktoolbar-bookmark" label="&bookmark;" hidden="true" class="menu-iconic"/>
          <menu id="linktoolbar-alternate" label="&alternate;" hidden="true" class="menu-iconic"/>
        </menupopup>
      </toolbarbutton>
    </statusbarpanel>
  </statusbar>
</overlay>
